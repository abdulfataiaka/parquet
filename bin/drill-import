#!/bin/bash

ROOTDIR="$(dirname $(cd $(dirname $0) && pwd))"
cd $ROOTDIR
source db/bin/config

tbnames="${*:1}"

if [ "$tbnames" == "--all" ]; then
  tbnames=$tables
fi

if [ "$tbnames" == "" ]; then
  printf "\n[*] No table has been specified for import\n\n"
  exit
fi

docker-compose run --rm spark bin/convert ${tbnames[*]}
echo
